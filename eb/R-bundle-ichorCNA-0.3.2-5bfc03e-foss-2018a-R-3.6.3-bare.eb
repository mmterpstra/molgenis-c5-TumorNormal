postinstallcmds = [ 'mkdir -p %(installdir)s/bin', 
    'wget https://raw.githubusercontent.com/broadinstitute/ichorCNA/5bfc03ed854f0e93fe5b624c97c1290fa0053837/scripts/createPanelOfNormals.R -O %(installdir)s/bin/createPanelOfNormals.R.tmp',
    'echo "#!/usr/bin/env Rscript " | ' + 
        'cat /dev/stdin %(installdir)s/bin/createPanelOfNormals.R.tmp > %(installdir)s/bin/createPanelOfNormals.R',
    'wget https://raw.githubusercontent.com/broadinstitute/ichorCNA/5bfc03ed854f0e93fe5b624c97c1290fa0053837/scripts/runIchorCNA.R -O %(installdir)s/bin/runIchorCNA.R.tmp',
    'echo "#!/usr/bin/env Rscript " | ' + 
         'cat /dev/stdin %(installdir)s/bin/runIchorCNA.R.tmp > %(installdir)s/bin/runIchorCNA.R',
    'rm %(installdir)s/bin/createPanelOfNormals.R.tmp  %(installdir)s/bin/runIchorCNA.R.tmp -v ' ,
    'chmod -R ugo+rx %(installdir)s/bin/*.R']


sanity_check_paths = {
    'files': ['bin/createPanelOfNormals.R','bin/runIchorCNA.R'],
    'dirs': ['bin', 'ichorCNA',],
}

moduleclass = 'bio'
